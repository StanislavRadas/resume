{{> title data=name big=true}}
<br>
<hr>
<main>
    <section>
        <div>
            <ul>
                {{#each users}}
                <li><b>Users:</b></li>
                <li>
                    <span class="badge text-bg-secondary">id: {{id}}</span>
                    <span class="badge text-bg-secondary">name: {{name}}</span>
                    <span class="badge text-bg-secondary">gender: {{gender}}</span>
                    <span class="badge text-bg-secondary">age: {{age}}</span>
                    <div>
                        <ul>
                            {{#each friends}}
                            <li><b>friends:</b></li>
                            <li>
                                <span class="badge text-bg-secondary">id: {{id}}</span>
